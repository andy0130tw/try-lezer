@precedence { times @left, plus @left }

@top { expression }
@skip { space }

expression {
  Number |
  BinaryExpression |
  "(" expression ")"
}

BinaryExpression {
  expression !times "*" expression |
  expression !plus "+" expression |
}

@tokens {
  space { std.whitespace+ }
  Number { std.digit+ }
}
